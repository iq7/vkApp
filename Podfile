# Uncomment the next line to define a global platform for your project
source 'https://github.com/CocoaPods/Specs.git'
platform :ios, '11.2'

target 'vkApp' do
  use_frameworks!
  pod 'Alamofire', '~> 4.7'
  pod 'SwiftyJSON'
  pod 'RealmSwift'
  pod 'Firebase/Core'
  pod 'Firebase/Database'
  pod 'SwiftKeychainWrapper'
end

target 'TodayVk' do
    use_frameworks!
    pod 'Alamofire', '~> 4.7'
    pod 'SwiftyJSON'
    pod 'RealmSwift'
end

target 'iMessage' do
    use_frameworks!
    pod 'Alamofire', '~> 4.7'
    pod 'SwiftyJSON'
    pod 'RealmSwift'
    pod 'SwiftKeychainWrapper'
end
